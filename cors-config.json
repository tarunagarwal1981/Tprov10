      "AllowedOrigins": [
        "http://localhost:3000",
        "https://*.amplifyapp.com",
        "https://*.netlify.app"
      ],
      "ExposeHeaders": ["ETag", "x-amz-server-side-encryption"],
      "MaxAgeSeconds": 3000
    }
  ]
}

